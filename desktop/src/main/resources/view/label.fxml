            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.columnIndex="2" GridPane.rowIndex="2" />
         </children>
      </GridPane>
      <HBox alignment="BOTTOM_CENTER" prefHeight="50.0" prefWidth="260.0" spacing="120.0">
         <children>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#back" text="%back" />
            <Button fx:id="nextButton" mnemonicParsing="false" onAction="#ok" text="%ok" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
